<?xml version="1.0" encoding="UTF-8" ?>
<messages>

    <message id="ww-template">
        <link id="wittywizardstylesheet" href="" type="text/css" rel="stylesheet" />
        <link id="wittywizardmenustylesheet" href="" type="text/css" rel="stylesheet" />
        <div id="page" style="padding:10px;">
            <div id="banner_wrapper">
                <div id="banner_content">
                    <div id="banner_end">
                        <div id="banner">
                            ${banner}
                        </div>
                    </div>
                </div>
            </div>

            <div id="main_wrapper" class="sfmenufixhorz">
                <div id="main_menu" class="sfmenumodhorz">
                    ${menu}
                </div>
                <div class="clearall"></div>
            </div>

            <div id="main_content" class="panel">
                <div id="content" class="panel-body">
                    ${contents}
                </div>
            </div>

            <div id="footer_wrapper" class="panel">
                <div id="footer_content" class="panel-footer">
                    <div id="footer_copyright" style="display: inline;">
                        ${copyright}
                    </div>

                    <div id="footer_menu" style="float: right; display: inline;">
                        ${footermenu}
                    </div>

                    <div id="hitcounterman">
                        ${hitcounterman}
                    </div>

                    <div class="clearall"></div>
                </div>
            </div>  <!-- id="footer_wrapper" -->
        </div> <!-- id="page" -->
        <div id="ga">
            ${ga}
        </div>
